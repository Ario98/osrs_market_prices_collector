{
  "dashboard": {
    "title": "OSRS Collector Health Monitor",
    "tags": ["osrs", "health", "monitoring"],
    "timezone": "utc",
    "refresh": "30s",
    "schemaVersion": 36,
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "templating": {
      "list": []
    },
    "annotations": {
      "list": [
        {
          "name": "Annotations & Alerts",
          "datasource": {
            "type": "grafana",
            "uid": "-- Grafana --"
          },
          "showIn": 0,
          "tags": [],
          "type": "dashboard"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Collector Status",
        "type": "stat",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "OSRS-TimescaleDB"
            },
            "rawSql": "SELECT collector_name, CASE status WHEN 'running' THEN 1 WHEN 'paused' THEN 2 WHEN 'error' THEN 3 ELSE 0 END as status_code, status FROM collector_state ORDER BY collector_name",
            "format": "table"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"options": {"0": {"text": "Unknown", "color": "gray"}}, "type": "value"},
              {"options": {"1": {"text": "Running", "color": "green"}}, "type": "value"},
              {"options": {"2": {"text": "Paused", "color": "yellow"}}, "type": "value"},
              {"options": {"3": {"text": "Error", "color": "red"}}, "type": "value"}
            ]
          }
        }
      },
      {
        "id": 2,
        "title": "Real-time Lag",
        "type": "gauge",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "OSRS-TimescaleDB"
            },
            "rawSql": "SELECT EXTRACT(EPOCH FROM (NOW() - MAX(timestamp))) / 60 as lag_minutes FROM prices WHERE timestamp > NOW() - INTERVAL '2 hours'",
            "format": "table"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 60,
            "unit": "m",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 15}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Records Last Hour",
        "type": "stat",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "OSRS-TimescaleDB"
            },
            "rawSql": "SELECT COUNT(*) as records FROM prices WHERE timestamp > NOW() - INTERVAL '1 hour'",
            "format": "table"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 4,
        "title": "Database Size",
        "type": "stat",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "OSRS-TimescaleDB"
            },
            "rawSql": "SELECT pg_database_size('osrs_market') as size_bytes",
            "format": "table"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "options": {
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        }
      },
      {
        "id": 5,
        "title": "Collection Rate (Last 24h)",
        "type": "timeseries",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "OSRS-TimescaleDB"
            },
            "rawSql": "SELECT time_bucket('5 minutes', inserted_at) as time, COUNT(*) as records_inserted FROM prices WHERE inserted_at > NOW() - INTERVAL '24 hours' GROUP BY time ORDER BY time",
            "format": "time_series"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 4},
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 6,
        "title": "Data Gaps (Last 24h)",
        "type": "table",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "OSRS-TimescaleDB"
            },
            "rawSql": "SELECT gap_start as \"Start Time\", gap_end as \"End Time\", duration_minutes as \"Duration (min)\", status as \"Status\", detected_at as \"Detected\" FROM data_gaps WHERE detected_at > NOW() - INTERVAL '24 hours' ORDER BY detected_at DESC LIMIT 50",
            "format": "table"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12}
      },
      {
        "id": 7,
        "title": "Missing Intervals (Last 6h)",
        "type": "timeseries",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "OSRS-TimescaleDB"
            },
            "rawSql": "WITH time_series AS (SELECT generate_series(date_trunc('hour', NOW() - INTERVAL '6 hours'), date_trunc('hour', NOW()), INTERVAL '5 minutes') as expected_time), actual_data AS (SELECT time_bucket('5 minutes', timestamp) as actual_time, COUNT(*) as record_count FROM prices WHERE timestamp > NOW() - INTERVAL '6 hours' GROUP BY actual_time) SELECT e.expected_time as time, CASE WHEN a.record_count IS NULL OR a.record_count < 50 THEN 1 ELSE 0 END as has_gap, COALESCE(a.record_count, 0) as record_count FROM time_series e LEFT JOIN actual_data a ON e.expected_time = a.actual_time ORDER BY time",
            "format": "time_series"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Error Rate (Last 7 Days)",
        "type": "timeseries",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "OSRS-TimescaleDB"
            },
            "rawSql": "SELECT time_bucket('1 hour', timestamp) as time, COUNT(*) as error_count FROM collection_metrics WHERE (api_status_code >= 400 OR api_error_message IS NOT NULL) AND timestamp > NOW() - INTERVAL '7 days' GROUP BY time ORDER BY time",
            "format": "time_series"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20}
      },
      {
        "id": 9,
        "title": "Recent Errors (Last 24h)",
        "type": "table",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "OSRS-TimescaleDB"
            },
            "rawSql": "SELECT timestamp as \"Time\", collector_name as \"Collector\", api_endpoint as \"Endpoint\", api_status_code as \"Status\", LEFT(api_error_message, 100) as \"Error Message\" FROM collection_metrics WHERE (api_status_code >= 400 OR api_error_message IS NOT NULL) AND timestamp > NOW() - INTERVAL '24 hours' ORDER BY timestamp DESC LIMIT 20",
            "format": "table"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20}
      },
      {
        "id": 10,
        "title": "Backfill Progress",
        "type": "gauge",
        "targets": [
          {
            "datasource": {
              "type": "postgres",
              "uid": "OSRS-TimescaleDB"
            },
            "rawSql": "SELECT COALESCE(progress_percentage, 0) as pct_complete, intervals_processed as completed, COALESCE(total_intervals_to_process, 0) as total, current_timestamp as current_position, target_timestamp as target FROM backfill_progress WHERE status = 'running' ORDER BY started_at DESC LIMIT 1",
            "format": "table"
          }
        ],
        "gridPos": {"h": 4, "w": 12, "x": 0, "y": 28},
        "options": {
          "showThresholdLabels": true
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent"
          }
        }
      }
    ]
  }
}
